  "folder_structure": {
    "description": "Recommended folder organization",
    "pattern": "cars/{car_id}/{category}/",
    "example": {
      "cars/": {
        "mclaren_p1_001/": {
          "images/": {
            "standard/": ["front.jpg", "rear.jpg", "side_left.jpg", "side_right.jpg"],
            "interior/": ["dashboard.jpg", "seats.jpg"],
            "details/": ["wheels.jpg", "engine.jpg"],
            "action/": ["track.jpg"]
          },
          "models/": ["car_model.glb"],
          "metadata.json": "Car-specific metadata"
        }
      }
    }
  },
  "usage_instructions": {
    "adding_new_car": [
      "1. Create unique car ID (e.g., brand_model_variant_number)",
      "2. Upload images to imgbb and get URLs",
      "3. Add car entry to this JSON with all image URLs",
      "4. Organize images by category (standard, interior, details, action)",
      "5. Optional: Add 3D model file path when available"
    ],
    "image_url_format": "https://i.ibb.co/[imgbb_id]/[filename].[ext]",
    "supported_formats": ["jpg", "jpeg", "png", "webp"],
    "recommended_resolution": "1920x1080 or higher for main views"
  }
}